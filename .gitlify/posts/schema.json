{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "Posts",
    "default": {},
    "required": ["userId", "id", "title", "body"],
    "properties": {
        "userId": {
            "$ref": "users#/properties/id"
        },
        "id": {
            "type": "integer"
        },
        "title": {
            "type": "string"
        },
        "body": {
            "type": "string"
        }
    },
    "additionalProperties": true
}
